@model Uye
@{
    ViewData["Title"] = "CreateUye";
    Layout = "~/Views/Default/Index.cshtml";
}

<div class="content">
	<div class="container-fluid">
		<h4 class="page-title">Üye</h4>
		<div class="row">
			<div class="col-md-12">
				<div class="card">
					<div class="card-header">
						<div class="card-title">Uye Ekleme Sayfası</div>
					</div>
					<div class="card-body">
						<form method="post">
							<label>AD-SOYAD</label>
							<input type="text" asp-for="AdSoyad" class="form-control" />
							<br />
							<label>Telefon</label>
							<input type="text" asp-for="Telefon" class="form-control" />
							<br />
							<label>Mail</label>
							<input type="text" asp-for="Email" class="form-control" />
							<br />
							<label>Tarih</label>
							<input type="text" asp-for="UyelikTarihi" class="form-control" />
							<label>ADRES</label>
							<input type="text" asp-for="Adres" class="form-control" />

							<br />
							<br />



							<button class="btn btn-primary">Uye Ekle</button>
						</form>

					</div>
				</div>
			</div>
		</div>
	</div>
</div>

